<%- include('header') %>


   
    <div class="container-fluid">
      <div class="card">
        <div class="card-body">
          <table>
            <thead>
              <tr>
                <th>quotes</th>
                <th>writer name</th>
                <th>delete</th>
                <th>edit</th>
              </tr>
            </thead>
            <tbody>
              <%alldata.forEach((val)=>{ %>
                  <tr>
                    <td><%=val.quotes%></td>
                    <td><%=val.writer%></td>
                    <td><a href="/admin/delete_quotes/<%=val.id%>" id="actionbutton">delete</a></td>
                    <td> <a href="/admin/edit_quotes/<%=val.id%>" id="actionbutton">edit</a></td>
                  </tr>
             <% }) %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
<script src="../assets/libs/jquery/dist/jquery.min.js"></script>
<script src="../assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="../assets/js/sidebarmenu.js"></script>
<script src="../assets/js/app.min.js"></script>
<script src="../assets/libs/simplebar/dist/simplebar.js"></script>
</body>
</html>